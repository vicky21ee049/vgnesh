#include <stdio.h>
int main()
{
    int n=12345;
    int e;
    int o;
    int es=0;
    int os=0;
    int r;
    
    while(n>0)
    {
        r=n%10;
        
        if(n%2==0)
        {
            es=es+r;
            e++;
        }
        else
        {
            os=os+r;
            o++;
        }
        n=n/10;
    }
    printf("even: %dOdd: %devensum: %dOddsum: %d",e,o,es,os);

}
